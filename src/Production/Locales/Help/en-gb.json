{
  "about": {
    "description": "Information about the bot",
    "usage": "about"
  },
  "feedback": {
    "description": "Tell me what do you think of the bot, report bugs, etc...",
    "usage": "feedback <message>",
    "examples": ["feedback Your bot is amazing!", "feedback There is a bug when we run the support command :x"]
  },
  "guide": {
    "description": "Get help on using main Homer features",
    "usage": "guide"
  },
  "help": {
    "description": "Get help on using the bot",
    "usage": "help"
  },
  "invite": {
    "description": "Invite the bot on your server",
    "usage": "invite"
  },
  "ping": {
    "description": "Pong!",
    "usage": "ping"
  },
  "support": {
    "description": "Join the support server to get help on using the bot",
    "usage": "support"
  },
  "vote": {
    "description": "Vote instructions and see who voted the most this month",
    "usage": "vote",
    "examples": []
  },

  "avatar": {
    "description": "Get the avatar of someone",
    "usage": "avatar [User]",
    "examples": ["avatar", "avatar b1nzy#1337"]
  },
  "channel": {
    "description": "Get information on a text channel",
    "usage": "channel [#channel]",
    "examples": ["channel", "channel #fornite"]
  },
  "checkdbans": {
    "description": "Checks if a user ID is listed on the Discord Bans list (unofficial)",
    "usage": "checkdbans ([User]|[UserID)",
    "examples": ["checkdbans", "checkdbans b1nzy#1337", "checkdbans 657385946273049384"]
  },
  "google": {
    "description": "Search for content on Google",
    "usage": "google <search>",
    "examples": ["google What's the best way to import a Python module ?"]
  },
  "lookup": {
    "description": "Search for guilds, users and invites through Discord",
    "usage": "lookup <(guildID|userID|invite)>",
    "examples": ["lookup 205427654042583040", "lookup 382951433378594817", "lookup https://discord.gg/Z9mKZV6"]
  },
  "role": {
    "description": "Get information about server roles",
    "usage": "role <role name>",
    "examples": ["role Admins"]
  },
  "server": {
    "description": "Get information about the server",
    "usage": "server"
  },
  "translate": {
    "description": "Translate the provided text into the given language",
    "usage": "translate <target language (ISO 639-1)> <text>",
    "examples": ["translate en Bonjour, comment allez-vous ?"]
  },
  "user": {
    "description": "Get information about someone",
    "usage": "user [User]",
    "examples": ["user", "user b1nzy#1337"]
  },
  "weather": {
    "description": "Get the current weather for the given location",
    "usage": "weather <location>",
    "examples": ["weather Paris, France"]
  },
  "youtube": {
    "description": "Search for content on YouTube",
    "usage": "youtube <search>",
    "examples": ["youtube Logan Paul amazing videos"]
  },

  "afk": {
    "description": "Marks you as absent (AFK)",
    "usage": "afk [reason]",
    "examples": ["afk I'm doing my homework, see you later!"]
  },
  "choose": {
    "description": "Select between the given choices",
    "usage": "choose <choice A> <choice B> [choice C] etc.",
    "examples": ["choose iDroid Artuto iBot Endless"]
  },
  "names": {
    "description": "Search for previous names of user",
    "usage": "names [@user]",
    "examples": ["names", "names @b1nzy"]
  },
  "poll": {
    "description": "Create polls and start voting!",
    "usage": "poll <title> [options]",
    "examples": ["poll What do you prefer ? -d A: McDonald's - B: Quick -e :regional_indicator_a: :regional_indicator_b: -t 1h -c GREEN"]
  },
  "profile": {
    "description": "Create or edit your profile, get people profiles or see what are the available fields",
    "usage": "profile (set <field name> <field content>|clear <field name>|<user>|list)",
    "examples": ["profile iDroid#4441", "profile list", "profile", "profile set twitter iDroid27210", "profile clear twitter"]
  },
  "quote": {
    "description": "Fetch a message using its ID and display it",
    "usage": "quote <message ID> [#channel]",
    "examples": ["quote 123456789", "quote 123456789 #super-secret-place"]
  },
  "remind": {
    "description": "Reminds you of tasks you set",
    "usage": "remind (<duration> <description>|list|remove <index>)",
    "examples": ["remind 1h Taking shower", "remind list", "remind remove 2"]
  },
  "say": {
    "description": "Repeat what you told to the bot",
    "usage": "say <text>",
    "examples": ["say I am the best bot ever created, pst.. it's wrong but don't tell anyone!"]
  },
  "tag": {
    "description": "Play with advanced custom commands!",
    "usage": "tag (subcommand <args>)|<tag name>",
    "examples": ["tag create <tagName> <tagContent>", "tag delete <tagName>", "tag edit <tagName> <newContent>", "tag owner <tagName>", "tag search <tagSearch>", "tag random", "tag raw <tagName>", "tag <tagName>"]
  },
  "timefor": {
    "description": "What time is it there ?",
    "usage": "timezone (set <timezone>)|([member])",
    "examples": ["timezone set Europe/Paris", "timezone iDroid#4441"]
  },

  "category": {
    "description": "Disable a command category for the server",
    "usage": "category <list|category name>",
    "examples": ["category list", "category Moderation"]
  },
  "format": {
    "description": "Set a date or a time format for the guild",
    "usage": "format <type> <format>",
    "examples": ["format date DD/MM/YYYY", "format time HH:mm:ss"]
  },
  "ignore": {
    "description": "Make the bot ignore or listen to a channel",
    "usage": "ignore [Channel]",
    "examples": ["ignore #trash", "ignore partOfChannelName"]
  },
  "language": {
    "description": "Set a locale for the guild",
    "usage": "language <code>",
    "examples": ["language en-gb", "language de-de", "language fr-fr"]
  },
  "leave": {
    "description": "Set-up the leave feed",
    "usage": "leave <message>",
    "examples": ["leave Oh no, {user.tag} has left!"]
  },
  "prefix": {
    "description": "Manage your custom prefixes",
    "usage": "prefix [prefix]",
    "examples": ["prefix bot!", "prefix homer::"]
  },
  "timezone": {
    "description": "Set a timezone for the guild",
    "usage": "timezone <timezone>",
    "examples": ["timezone Europe/Paris", "timezone America/Mexico_City"]
  },
  "welcome": {
    "description": "Set-up the welcome feed",
    "usage": "welcome <message>",
    "examples": ["welcome Oh yeah, {user.tag} just entered the place!"]
  },

  "call": {
    "description": "Call a telephone number",
    "usage": "call <number>",
    "examples": ["call 111-222"]
  },
  "hangup": {
    "description": "Hangup a call",
    "usage": "hangup"
  },
  "number": {
    "description": "Shows your telephone number",
    "usage": "number [reset]",
    "examples": ["number", "number reset"]
  },
  "pickup": {
    "description": "Pickup a pending call",
    "usage": "pickup",
    "examples": []
  },
  "phmessage": {
    "description": "Set or reset messages that are used by the telephone.",
    "usage": "phmessage <type> <text>",
    "examples": ["phmessage incoming Hey, @b1nZy you receive a call", "phmessage missed We will re-call you soon"]
  },
  "phonebook": {
    "description": "Search for telephone numbers in the phonebook",
    "usage": "phonebook <switch|random|phone number|blacklist <phone number>>",
    "examples": ["phonebook switch", "phonebook random", "phonebook 1-464-393", "phonebook blacklist 999-999"]
  },
  "setphone": {
    "description": "Set-up the telephone channel or reset it",
    "usage": "setphone [#channel] or clear",
    "examples": ["setphone", "setphone #my-telephone", "setphone clear"]
  },

  "ban": {
    "description": "Bans a member from the server",
    "usage": "ban <@user|ID> [reason]",
    "examples": ["ban @iDroid", "ban @b1nzy Not a nice guy (jk)"]
  },
  "case": {
    "description": "Get infomation about a moderation case",
    "usage": "case <case ID>",
    "examples": ["case 3"]
  },
  "clear": {
    "description": "Clear messages using the given criteria",
    "usage": "clear (num|bots|links|files|@user|ID|\"string\"|`expression`) [reason]",
    "examples": ["clear 1", "clear \"fuck\"", "clear @iDroid Stop that please"]
  },
  "hackban": {
    "description": "Hackbans a member from the server",
    "usage": "hackban <ID> [reason]",
    "examples": ["hackban 1234", "hackban 1234 Get out of here"]
  },
  "kick": {
    "description": "Kicks a member from the server",
    "usage": "kick <@user|ID> [reason]",
    "examples": ["kick @iDroid", "kick @b1nzy Not a nice guy (jk)"]
  },
  "modlog": {
    "description": "Set-up a modlog channel or reset it",
    "usage": "modlog [#channel] or <clear>",
    "examples": ["modlog", "modlog #moderation", "modlog clear"]
  },
  "reason": {
    "description": "Edits the reason of a moderation case",
    "usage": "reason <case ID> <reason>",
    "examples": ["reason 4 Actually it's because he wasn't clever"]
  },
  "tempban": {
    "description": "Bans a member from the server for the specified time",
    "usage": "tempban <duration> <@user|ID> [reason]",
    "examples": ["tempban 1d @b1nzy Calm down"]
  },
  "unban": {
    "description": "Unbans a user from the server",
    "usage": "unban <Tag|ID> [reason]",
    "examples": ["unban b1nzy#1337 He was nice actually", "unban 1234"]
  }
}